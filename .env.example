# Deployment mode
APP_ENV=production
SOLANA_NETWORK=mainnet-beta
SOLANA_RPC=https://api.mainnet-beta.solana.com
# Local preview can proxy /api/* to this deployed base URL.
APP_BASE_URL=https://your-vercel-domain.vercel.app

# Wallet adapter configuration
WALLET_ADAPTER_IDENTITY=SeekerMigrate
WALLET_ADAPTER_RPC=https://rpc-wallet-adapter.example.com

# Payment rails (Solana Pay/api key placeholders)
PAYMENTS_PROVIDER=solana-pay
PAYMENTS_API_KEY=
PAYMENTS_ENDPOINT=/api/payments
PAYMENT_MERCHANT_ADDRESS=EnterYourMerchantPublicKey
PAYMENT_MERCHANT_LABEL=Seeker Store

# Stripe (card rails)
STRIPE_SECRET_KEY=
STRIPE_SUCCESS_URL=https://your-domain/payments/success?session_id={CHECKOUT_SESSION_ID}
STRIPE_CANCEL_URL=https://your-domain/

# Vanity wallet generator (service credentials)
VANITY_SERVICE_URL=/api/vanity
VANITY_API_KEY=
VANITY_COST_LAMPORTS=50000
VANITY_MAX_ATTEMPTS=25000

# Name service (SNS or solana-based naming)
NAME_SERVICE_RPC=/api/name
NAME_SERVICE_API_KEY=
NAME_ACCOUNT_SPACE=2000

# Telegram bot integration
TELEGRAM_BOT_TOKEN=bot123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11
TELEGRAM_WEBHOOK_SECRET=replace-with-a-random-string
TELEGRAM_ADMIN_CHAT_ID=1234567890

# Supabase content (optional)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Optional observability
LOG_LEVEL=info
SENTRY_DSN=
