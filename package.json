{
  "name": "seekermigrate",
  "version": "0.1.0",
  "description": "CLI tool to convert Firebase Email Authentication to Solana Seeker wallet authentication",
  "main": "dist/index.js",
  "bin": {
    "seekermigrate": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup",
    "dev": "tsup --watch",
    "dev:web": "npm --prefix apps/web run dev",
    "build:web": "npm --prefix apps/web run build",
    "preview:web": "npm --prefix apps/web run preview",
    "start": "node dist/cli.js",
    "preview:local": "node scripts/local-preview.mjs",
    "telegram:start": "node dist/telegram.js",
    "dev:vanity-worker": "tsx watch src/server/vanity-worker/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "lint": "eslint src/ --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "dev:api": "tsx watch api/_dev/server.ts"
  },
  "keywords": [
    "solana",
    "seeker",
    "firebase",
    "migration",
    "wallet",
    "authentication",
    "react-native",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@bonfida/spl-name-service": "^2.5.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "@supabase/supabase-js": "^2.93.2",
    "bs58": "^6.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.20.2",
    "glob": "^10.3.10",
    "mustache": "^4.2.0",
    "ora": "^8.0.1",
    "stripe": "^20.2.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/express": "^5.0.6",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vercel/node": "^5.0.2",
    "eslint": "^8.54.0",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
